        <activity
            android:name=".FAQ.Contacts"
            android:label="Staff Contact Information">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staff.staffapp.FAQ.FAQ" />
        </activity>
        <activity
            android:name=".FAQ.FAQ"
            android:label="Frequently Asked Questions">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staff.staffapp.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
