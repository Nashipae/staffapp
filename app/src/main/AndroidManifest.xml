        <activity android:name=".FAQ.Panel" />
        <activity
            android:name=".FAQ.Medical"
            android:label="Medical Specialists">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staff.staffapp.FAQ.FAQ" />
        </activity>
        <activity
            android:name=".FAQ.Emergency"
            android:label="Emergency Contacts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.staff.staffapp.FAQ.FAQ" />
        </activity>
        <activity
            android:name=".FAQ.Leave"
            android:label="Leave Days">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
